#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/drizzle-kit@0.21.4/node_modules/drizzle-kit/node_modules:/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/drizzle-kit@0.21.4/node_modules:/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/drizzle-kit@0.21.4/node_modules/drizzle-kit/node_modules:/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/drizzle-kit@0.21.4/node_modules:/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/drizzle-kit@0.21.4/node_modules/drizzle-kit/bin.cjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/drizzle-kit@0.21.4/node_modules/drizzle-kit/bin.cjs" "$@"
fi
