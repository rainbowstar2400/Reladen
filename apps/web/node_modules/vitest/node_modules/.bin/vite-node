#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/vite-node@1.5.2_@types+node@20.11.30/node_modules/vite-node/node_modules:/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/vite-node@1.5.2_@types+node@20.11.30/node_modules:/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/vite-node@1.5.2_@types+node@20.11.30/node_modules/vite-node/node_modules:/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/vite-node@1.5.2_@types+node@20.11.30/node_modules:/mnt/c/Users/cs24035/Documents/develop/Reladen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-node@1.5.2_@types+node@20.11.30/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../vite-node@1.5.2_@types+node@20.11.30/node_modules/vite-node/vite-node.mjs" "$@"
fi
